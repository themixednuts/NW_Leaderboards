#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/browserslist@4.21.5/node_modules/browserslist/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/browserslist@4.21.5/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/node_modules:/mnt/c/Users/jonfo/Documents/node_modules:/mnt/c/Users/jonfo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jonfo/Documents/Github/Leaderboards/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/browserslist@4.21.5/node_modules/browserslist/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/browserslist@4.21.5/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules/.pnpm/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/Leaderboards/node_modules:/mnt/c/Users/jonfo/Documents/GitHub/node_modules:/mnt/c/Users/jonfo/Documents/node_modules:/mnt/c/Users/jonfo/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jonfo/Documents/Github/Leaderboards/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
